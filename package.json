{
  "name": "extension",
  "displayName": "Extension",
  "version": "0.0.1",
  "private": true,
  "description": "a Chrome extension",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background build:js",
    "release": "node scripts/release.js",
    "pack:crx": "node scripts/pack-crx.js",
    "pack:zip": "node scripts/pack-zip.js",
    "pack:xpi": "node scripts/pack-xpi.js",
    "dev-firefox": "npm run clear && cross-env NODE_ENV=development EXTENSION=firefox run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.mts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.mts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "packxpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./dist web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "lint": "eslint --cache .",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "up": "taze major -I",
    "commit": "bumpp --no-commit --no-push --no-tag && git add package.json && czg"
  },
  "dependencies": {
    "@iconify-json/ep": "^1.2.1",
    "alova": "^3.2.6",
    "cash-dom": "^8.1.5",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@ffflorian/jszip-cli": "^3.8.2",
    "@iconify/json": "^2.2.279",
    "@playwright/test": "^1.49.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@unocss/reset": "^0.65.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.0.0",
    "ali-oss": "^6.22.0",
    "archiver": "^7.0.1",
    "bumpp": "^10.0.2",
    "chokidar": "^4.0.1",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "czg": "^1.11.0",
    "element-plus": "^2.9.0",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^25.0.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.82.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unocss": "^0.65.1",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-icons": "^0.21.0",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^6.0.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10",
    "web-ext": "^8.3.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  }
}
