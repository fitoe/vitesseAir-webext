{
  "cSpell.words": [
    "Vitesse"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "vite.autoStart": false,
  "eslint.useFlatConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "files.exclude": {
    "**/.classpath": false,
    "**/.project": false,
    "**/.settings": false,
    "**/.factorypath": false
  }
}
